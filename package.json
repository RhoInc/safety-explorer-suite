    "safety-histogram":"~2",
    "safety-results-over-time":"~2",
    "safety-shift-plot":"~2",
    "safety-outlier-explorer":"~2"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-preset-es2015-rollup": "^1.1.1",
