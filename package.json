  "license": "MIT",
  "dependencies": {
    "d3": "^3.5.14",
    "webcharts":"1.9.0",
    "aeexplorer":"3.2.2",
    "ae-timelines":"2.0.3",
    "safety-histogram":"2.0.3",
    "safety-results-over-time":"2.0.3",
    "safety-shift-plot":"2.0.3",
    "safety-outlier-explorer":"2.0.3",
    "paneled-outlier-explorer":"1.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-preset-es2015-rollup": "^1.1.1",
